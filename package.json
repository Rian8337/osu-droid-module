{
  "name": "osu-droid",
  "version": "1.0.0",
  "description": "A module containing utilities for osu!droid and osu! in general.",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format-code": "npx prettier -w --loglevel=silent \"src/**\"",
    "prepare": "dts-gen -m java-deserialization -o -f ./node_modules/java-deserialization/src/index.d.ts && npm run build"
  },
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "osu!droid",
    "osu!"
  ],
  "files": ["dist/**", "typings/**"],
  "author": "Rian8337",
  "license": "ISC",
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^17.0.0",
    "@types/request": "^2.48.7",
    "@types/unzipper": "^0.10.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "dts-gen": "^0.6.0",
    "eslint": "^8.5.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "canvas": "^2.8.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "java-deserialization": "^0.1.0",
    "lodash.clonedeep": "^4.5.0",
    "request": "^2.88.2",
    "unzipper": "^0.10.11"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "prettier": {
    "tabWidth": 4
  }
}
